 #define AID_EVERYBODY     9997  /* shared between all apps in the same profile */
 #define AID_MISC          9998  /* access to misc storage */
@@ -172,6 +173,7 @@ static const struct android_id_info android_ids[] = {
 
     { "everybody",     AID_EVERYBODY, },
     { "misc",          AID_MISC, },
