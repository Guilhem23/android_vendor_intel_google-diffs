From d42aa3cbe150029ce8a19d0f78853f52929d3e0f Mon Sep 17 00:00:00 2001
From: Yong Yao <yong.yao@intel.com>
Date: Mon, 24 Feb 2014 15:53:54 +0800
Subject: [PATCH] Tmp Disable components to pass build

Change-Id: Id75e6e9ace8e9b93abf326b101f3f33d556731f6
Signed-off-by: Yong Yao <yong.yao@intel.com>
---
 BoardConfig.mk         |    2 +-
 saltbay/BoardConfig.mk |    8 ++++----
 saltbay/device.mk      |   41 ++++++++++++++++++++---------------------
 3 files changed, 25 insertions(+), 26 deletions(-)

diff --git a/BoardConfig.mk b/BoardConfig.mk
index b074cb3..ad78afb 100755
--- a/BoardConfig.mk
+++ b/BoardConfig.mk
@@ -4,7 +4,7 @@
 TARGET_OS_SIGNING_METHOD := xfstk_no_xml
 include device/intel/common/BoardConfig.mk
 
-TARGET_ARCH_VARIANT := x86-slm
+TARGET_ARCH_VARIANT := silvermont
 
 TARGET_USE_DROIDBOOT := true
 
diff --git a/saltbay/BoardConfig.mk b/saltbay/BoardConfig.mk
index f4eac29..b2fc45d 100644
--- a/saltbay/BoardConfig.mk
+++ b/saltbay/BoardConfig.mk
@@ -48,15 +48,15 @@ FLASHFILE_NO_OTA := false
 BOARD_HAVE_WIFI := true
 
 # WIDI
-INTEL_WIDI := true
-INTEL_WIDI_MERRIFIELD := true
+INTEL_WIDI := false
+INTEL_WIDI_MERRIFIELD := false
 TARGET_FORCE_HWC_FOR_VIRTUAL_DISPLAYS := true
 
 BOARD_USES_VIDEO := true
 BOARD_USES_ALSA_AUDIO := true
 BOARD_USES_TINY_ALSA_AUDIO := true
 BOARD_USES_AUDIO_HAL_XML := true
-BOARD_USE_VIBRATOR := true
+BOARD_USE_VIBRATOR := false
 ifneq (,$(filter $(TARGET_BUILD_VARIANT),eng userdebug))
 # Enable ALSA utils for eng and user debug builds
 BUILD_WITH_ALSA_UTILS := true
@@ -152,7 +152,7 @@ IA_PANORAMA_VERSION := 1.1
 
 USE_INTEL_HDMI  := false
 BOARD_USES_FORCE_SET_DISPLAY_BGRA_8888 := true
-TARGET_HAS_MULTIPLE_DISPLAY := true
+TARGET_HAS_MULTIPLE_DISPLAY := false
 TARGET_HAS_VPP := true
 COMMON_GLOBAL_CFLAGS += -DGFX_BUF_EXT
 
diff --git a/saltbay/device.mk b/saltbay/device.mk
index 4e9dec5..de7aeee 100644
--- a/saltbay/device.mk
+++ b/saltbay/device.mk
@@ -58,7 +58,7 @@ PRODUCT_PACKAGES += \
     libmamgr-xmm
 
 #hdmi audio
-PRODUCT_PACKAGES += \
+#PRODUCT_PACKAGES += \
     audio.hdmi.$(PRODUCT_DEVICE)
 
 #usb dock audio
@@ -75,11 +75,11 @@ PRODUCT_COPY_FILES += \
 	$(AUDIO_FW_PATH)/geq_lib_119a.bin:system/etc/firmware/geq_lib_119a.bin
 
 #For Audio Offload support
-PRODUCT_PACKAGES += \
+#PRODUCT_PACKAGES += \
     audio.codec_offload.$(PRODUCT_DEVICE)
 
 #NXP audio effects
-PRODUCT_PACKAGES += \
+#PRODUCT_PACKAGES += \
     libbundlewrapper.so \
     libreverbwrapper.so \
     libxmlparser.so \
@@ -88,7 +88,7 @@ PRODUCT_PACKAGES += \
     audio_effects.conf
 
 #Effects Offload
-PRODUCT_PACKAGES += \
+#PRODUCT_PACKAGES += \
     libeffectproxy \
     libswwrapper \
     libhwwrapper
@@ -106,12 +106,12 @@ PRODUCT_COPY_FILES += \
     $(DEVICE_CONF_PATH)/CsmConfig.xml:system/vendor/etc/CsmConfig.xml
 
 # Enable ALAC
-PRODUCT_PACKAGES += \
+#PRODUCT_PACKAGES += \
     libstagefright_soft_alacdec
 
 
 # AT Proxy
-PRODUCT_PACKAGES += \
+#PRODUCT_PACKAGES += \
     proxy
 
 # Restricted Access Region
@@ -273,15 +273,15 @@ PRODUCT_PACKAGES += \
     libstagefrighthw
 
 #widi audio HAL
-PRODUCT_PACKAGES += \
+#PRODUCT_PACKAGES += \
 audio.widi.$(PRODUCT_DEVICE)
 
 #remote submix audio
-PRODUCT_PACKAGES += \
+#PRODUCT_PACKAGES += \
     audio.r_submix.default
 
 # widi
-PRODUCT_PACKAGES += \
+#PRODUCT_PACKAGES += \
     widi
 
 PRODUCT_PACKAGES_DEBUG += \
@@ -367,12 +367,12 @@ PRODUCT_PACKAGES += \
     com.android.future.usb.accessory
 
 #OemTelephony for OEM HOOK API
-PRODUCT_PACKAGES += \
+#PRODUCT_PACKAGES += \
     com.intel.internal.telephony.OemTelephony \
      com.intel.internal.telephony.OemTelephony.xml
 
 #SARManager
-PRODUCT_PACKAGES += \
+#PRODUCT_PACKAGES += \
     com.intel.internal.telephony.SARManager \
     com.intel.internal.telephony.SARManager.xml
 
@@ -450,36 +450,35 @@ PRODUCT_COPY_FILES += \
 # parameter-framework libraries
 PRODUCT_PACKAGES += \
     libuta-subsystem \
-    libdsp-subsystem \
     libpower-subsystem
 
 # IMC's Audio Driver port on IA
 # # AUD_NVM_BASEDIR_CFLAG is used in the compilation of libaudwrapper (aka
 # aud_dependencies)
 # Warning: If you change this value, you must also update init.saltbay.rc
-AUD_NVM_BASEDIR_CFLAG := /factory/aud_nvm
-PRODUCT_PACKAGES += \
+#AUD_NVM_BASEDIR_CFLAG := /factory/aud_nvm
+#PRODUCT_PACKAGES += \
     libauddriver \
     libaudwrapper \
     libutaos
 
 # GTI
-PRODUCT_PACKAGES += \
+#PRODUCT_PACKAGES += \
     libgti \
     libgtisrv \
     libgtiproxy
 
 # libdsp
-PRODUCT_PACKAGES += \
+#PRODUCT_PACKAGES += \
     libdsp \
     lpeexplorer
 
 # LPE probing tools
-PRODUCT_PACKAGES_DEBUG += \
+#PRODUCT_PACKAGES_DEBUG += \
     prober
 
 # parameter-framework files
-PRODUCT_PACKAGES += \
+#PRODUCT_PACKAGES += \
     parameter-framework.audio.saltbay \
     parameter-framework.vibrator.merrifield
 
@@ -502,7 +501,7 @@ PRODUCT_PACKAGES_DEBUG += \
     mmgr-debug
 
 # CSM
-PRODUCT_PACKAGES += \
+#PRODUCT_PACKAGES += \
     CWS_SERVICE_MANAGER
 
 # TelephonyEventsNotifier
@@ -551,11 +550,11 @@ include $(PLATFORM_PATH)/merrifield.mk
 -include $(PLATFORM_CONF_PATH)/libdsp/AndroidLibdsp.mk
 
 #Aware
-PRODUCT_PACKAGES += \
+#PRODUCT_PACKAGES += \
     aware_bundle
 
 # build the OMX wrapper codecs
-PRODUCT_PACKAGES += \
+#PRODUCT_PACKAGES += \
     libstagefright_soft_mp3dec_mdp \
     libstagefright_soft_aacdec_mdp \
     libstagefright_soft_amrdec_mdp \
