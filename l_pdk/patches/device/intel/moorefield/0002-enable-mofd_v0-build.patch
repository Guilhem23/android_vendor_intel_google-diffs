diff --git a/mofd_v1/BoardConfig.mk b/mofd_v1/BoardConfig.mk
index 59e1740..53ffc1f 100644
--- a/mofd_v1/BoardConfig.mk
+++ b/mofd_v1/BoardConfig.mk
@@ -86,7 +86,7 @@ USE_INTEL_IPP := true
 # - MFX_IPP: sets IPP library optimization to use
 USE_MEDIASDK := true
 # Enable CIP Codecs
-USE_INTEL_MDP := true
+USE_INTEL_MDP := false
 MFX_IPP := p8
 # enabled to use Intel video accelerator
