+#PRODUCT_PACKAGES += \
     parameter-framework.audio.mofd_v1 \
     parameter-framework.vibrator.moorefield \
     parameter-framework.routeMgr.mofd_v1
@@ -550,5 +550,5 @@ PRODUCT_PACKAGES += \
     com.intel.ir94.cve.xml
 
