From 4eac770286002085b4bc2cabb0eb4129026182c8 Mon Sep 17 00:00:00 2001
From: Laurent FERT <laurent.fert@intel.com>
Date: Wed, 23 Jul 2014 09:22:50 +0200
Subject: [PATCH] [PATCH] Disable more features for mofd_v1 to build

Change-Id: Ic5e04b030ee9e1eedb8fa0c0b818c9f1a5e68b2e
Signed-off-by: Laurent FERT <laurent.fert@intel.com>
---
 mofd_v1/BoardConfig.mk |    9 +++++----
 mofd_v1/device.mk      |    4 ++--
 mofd_v1/mofd_v1.mk     |    8 ++++----
 moorefield.mk          |    2 +-
 4 files changed, 12 insertions(+), 11 deletions(-)

diff --git a/mofd_v1/BoardConfig.mk b/mofd_v1/BoardConfig.mk
index 619955b..31a5ff4 100644
--- a/mofd_v1/BoardConfig.mk
+++ b/mofd_v1/BoardConfig.mk
@@ -140,8 +140,8 @@ NEED_REMOVE_PADDING_BYTE := true
 # Set USE_CAMERA_STUB to 'true' for Fake Camera builds,
 # 'false' for libcamera builds to use Camera Imaging(CI) supported by intel.
 BOARD_CAMERA_IPU2_SUPPORT := true
-USE_CAMERA_STUB := false
-USE_CAMERA_HAL2 := true
+USE_CAMERA_STUB := true
+USE_CAMERA_HAL2 := false
 
 USE_CSS_2_1 := true
 USE_INTEL_METABUFFER := true
@@ -196,7 +196,8 @@ DEBUG_KERNEL_CMDLINE := loglevel=0
 endif
 BOARD_KERNEL_CMDLINE := init=/init pci=noearly $(DEBUG_KERNEL_CMDLINE) vmalloc=256M \
                         androidboot.hardware=$(TARGET_DEVICE) $(cmdline_extra) \
-                        ip=50.0.0.2:50.0.0.1::255.255.255.0::usb0:on debug_locks=0
+                        ip=50.0.0.2:50.0.0.1::255.255.255.0::usb0:on debug_locks=0 \
+                        selinux=0
 endif
 
 #enable Wide Band Speech support for HFP profile
@@ -236,4 +237,4 @@ SENSORHUB_FW_NAME_VALIDATION3 := 0000.0000.0008.0008.0001.0000
 ENABLE_IR94 := true
 
 # Adaptive Authentication
-INTEL_FEATURE_ADAPTIVE_AUTHENTICATION := true
+#INTEL_FEATURE_ADAPTIVE_AUTHENTICATION := true
diff --git a/mofd_v1/device.mk b/mofd_v1/device.mk
index 1a2aef1..df7e01b 100644
--- a/mofd_v1/device.mk
+++ b/mofd_v1/device.mk
@@ -45,7 +45,7 @@ PRODUCT_PACKAGES += \
     ImsProvider \
     ims_settings.xml
 
-PRODUCT_PACKAGES_DEBUG += VTDialer
+#PRODUCT_PACKAGES_DEBUG += VTDialer
 
 # keypad key mapping
 PRODUCT_COPY_FILES += \
@@ -438,7 +438,7 @@ PRODUCT_COPY_FILES += \
 PRODUCT_PACKAGES += libtinyalsa_custom-subsystem
 
 # SST probing tools
-PRODUCT_PACKAGES_DEBUG += \
+#PRODUCT_PACKAGES_DEBUG += \
     tinyprobe
 
 # Tuning files
diff --git a/mofd_v1/mofd_v1.mk b/mofd_v1/mofd_v1.mk
index bf47f00..fa2c75d 100644
--- a/mofd_v1/mofd_v1.mk
+++ b/mofd_v1/mofd_v1.mk
@@ -40,7 +40,7 @@ PRODUCT_PACKAGES += \
 
 # NFC - only if modem support enabled
 ifeq ($(BOARD_HAVE_MODEM), true)
-PRODUCT_PACKAGES += \
+#PRODUCT_PACKAGES += \
     nfc_pn547 \
     android.hardware.nfc.hce
 
@@ -50,7 +50,7 @@ PRODUCT_PROPERTY_OVERRIDES += \
     ro.nfc.clk=pll
 
 # SmartCard API
-PRODUCT_PACKAGES += \
+#PRODUCT_PACKAGES += \
    smartcard_api
 endif
 
@@ -61,7 +61,7 @@ endif
     libwebrtc_full \
     libjingle_full
 
-PRODUCT_PACKAGES_DEBUG += \
+#PRODUCT_PACKAGES_DEBUG += \
     WebRTCDemo
 
 # crashreport
@@ -74,7 +74,7 @@ PRODUCT_COPY_FILES += \
 # PRODUCT_PACKAGE_OVERLAYS := $(DEVICE_CONF_PATH)/overlays
 
 # Power Profile app
-PRODUCT_PACKAGES += \
+#PRODUCT_PACKAGES += \
     PowerSaver
 
 # Include base makefile
diff --git a/moorefield.mk b/moorefield.mk
index 3a30946..ff663d5 100644
--- a/moorefield.mk
+++ b/moorefield.mk
@@ -63,7 +63,7 @@ PRODUCT_PACKAGES += \
 
 # Temp disable for bringup
 # libwrs_omxil
-PRODUCT_PACKAGES += \
+#PRODUCT_PACKAGES += \
     libwrs_omxil_base \
     libwrs_omxil_common \
     libwrs_omxil_core_pvwrapped \
-- 
