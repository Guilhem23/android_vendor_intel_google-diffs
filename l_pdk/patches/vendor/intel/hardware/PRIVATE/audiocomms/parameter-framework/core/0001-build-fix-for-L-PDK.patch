-####################
-# Common definitions
-
-common_src_files := \
-    main.cpp \
-    TestPlatform.cpp
-
-common_module := test-platform
-common_module_tags := optional
-
-common_c_includes := \
-    $(LOCAL_PATH)/../../parameter/include \
-    $(LOCAL_PATH)/../../remote-processor/
-
-common_shared_libraries := libparameter libremote-processor
